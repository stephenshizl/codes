#!/bin/bash

OUT_HOME=/home/chengdongdong/work/tegra_x/out/target/product/macallan

if [ $# -lt 1 ] ; then
	echo "copy error!"
	exit -1;
fi

case $1 in
	bt)
		echo "acopy OUT $OUT_HOME/boot.img"
		shift 1
		;;
	rc)
		echo "acopy OUT $OUT_HOME/recovery.img"
		shift 1
		;;
esac

acopy OUT $OUT_HOME/bootloader.bin
